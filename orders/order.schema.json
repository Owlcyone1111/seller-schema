{
    "title": "Order",
    "type": "object",
    "properties": {
        "id": {
            "type": "string",
            "description" :  "Jet's unique ID for a given order"
        },
        "placed_on": {
            "type": "string",
            "description" :  "The date the order was placed in UTC"
        },
        "buyer": {
            "$ref": "#/definitions/buyer"
        },
        "shipping_to": {
            "$ref": "#/definitions/shipping_address"
        },
        "items": {
            "type": "array",
            "items": {
                "$ref": "#/definitions/item"
            }
        },
        "shipments": {
            "type": "array",
            "items": {
                "$ref": "#/definitions/shipment"
            }
        },
        "metadata": {
            "$ref": "#/definitions/metadata"
        }
    },
    "definitions": {
        "buyer": {
            "title": "Buyer",
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "phone_number": {
                    "type": "string"
                }
            }
        },
        "address": {
            "title": "Address",
            "type": "object",
            "properties": {
                "address1": {
                    "type": "string"
                },
                "address2": {
                    "type": "string"
                },
                "city": {
                    "type": "string"
                },
                "state": {
                    "type": "string"
                },
                "zip_code": {
                    "type": "string"
                }
            }
        },
        "shipping_address": {
            "title": "Shipping Address",
            "type": "object",
            "all_of": [
                {
                    "$ref": "#/definitions/buyer"
                },
                {
                    "$ref": "#/definitions/address"
                }
            ]
        },
        "item": {
            "title": "Item",
            "type": "object",
            "properties": {
                "id": {
                    "type": "string"
                },
                "merchant_sku": {
                    "type": "string"
                },
                "quantity": {
                    "type": "integer"
                },
                "tax_code": {
                    "type": "string"
                },
                "price": {
                    "type": "number"
                },
                "fees": {
                    "type": "number"
                },
                "fee_adjustments": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/fee_adjustment"
                    }
                },
                "name": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "url": {
                    "type": "string"
                }
            }
        },
        "shipment": {
            "title": "shipment",
            "type": "object",
            "properties": {
                "id": {
                    "type": "string"
                },
                "fulfillment_center": {
                    "$ref": "#/definitions/fulfillment_center"
                },
                "method": {
                    "$ref": "#/definitions/shipping_method"
                },
                "ship_by": {
                    "type": "string"
                },
                "delivery_by": {
                    "type": "string"
                },
                "box": {
                    "$ref": "#/definitions/shipping_box"
                },
                "items": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/shipping_item"
                    }
                },
                "label_url": {
                    "type": "string"
                }
            }
        },
        "fee_adjustment": {
            "title": "FeeAdjustment",
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "value": {
                    "type": "number"
                },
                "reason": {
                    "type": "string"
                }
            }
        },
        "shipping_box": {
            "title": "ShippingBox",
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "width": {
                    "type": "number"
                },
                "height": {
                    "type": "number"
                },
                "length": {
                    "type": "number"
                },
                "max_weight": {
                    "type": "number"
                },
                "url": {
                    "type": "string"
                }
            }
        },
        "fulfillment_center": {
            "title": "FulfillmentCenter",
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "url": {
                    "type": "string"
                }
            }
        },
        "shipping_method": {
            "title": "ShippingMethod",
            "type": "string"
        },
        "shipping_item": {
            "title": "ShippingItem",
            "type": "object",
            "properties": {
                "id": {
                    "type": "string"
                },
                "merchant_sku": {
                    "type": "string"
                },
                "quantity": {
                    "type": "integer"
                }
            }
        },
        "metadata": {
            "title": "Metadata",
            "type": "object",
            "properties": {
                "notified_on": {
                    "type": "string"
                },
                "state": {
                    "type": "object"
                },
                "history": {
                    "type": "array"
                }
            }
        }
    }
}