{
    "title": "Merchant_Order",
    "type": "object",
    "properties": {
        "merchant_order_id": {
            "type": "string",
            "required": true,
            "description" :  "Jet's unique ID for a given merchant order"
        },
        "order_placed_date": {
            "type": "string",
            "required": true,
            "description" :  "The date the merchant order was placed"
        },
        "Order_Transmission_Date":{
            "type": "string",
            "required": true,
            "description" : "The date/time the merchant order was sent to the merchant"
        },


        "buyer": {
            "$ref": "#/definitions/buyer"
        },
        "shipping_to": {
            "$ref": "#/definitions/shipping_address"
        },
        "items": {
            "type": "array",
            "items": {
                "$ref": "#/definitions/item"
            }
        },
        "order_detail": {
            "$ref": "#/definitions/order_detail"
        },
        "metadata": {
            "$ref": "#/definitions/metadata"
        }
    },
    "definitions": {
        "buyer": {
            "title": "Buyer",
            "type": "object",
            "properties": {
                "buyer_name": {
                    "type": "string",
                    "required": true,
                    "description" : "Name of the customer"
                },
                "buyer_phone_number": {
                    "type": "string",
                    "required": true,
                    "description" : "Phone number of the customer"
                }
            }
        },
        "address": {
            "title": "Address",
            "type": "object",
            "properties": {
                "shipping_address1": {
                    "type": "string",
                    "required": true,
                    "description" : "Line 1 of the shipping address"
                },
                "shipping_address2": {
                    "type": "string", 
                    "required": false,
                    "description" : "line 2 of the shipping address"
                },
                "shipto_city": {
                    "type": "string",
                    "required": true,
                    "description" : "Shipping address city"
                },
                "shipto_state": {
                    "type": "string",
                    "required": true,
                    "description" : "shipping address state"
                },
                "shipto_zip_code": {
                    "type": "string",
                    "required": true,
                    "description" : "shipping address zip code"
                }
            }
        },
        "shipping_address": {
            "title": "Shipping_Address",
            "type": "object",
            "all_of": [
                {
                    "$ref": "#/definitions/buyer"
                },
                {
                    "$ref": "#/definitions/address"
                }
            ]
        },
        "item": {
            "title": "Item",
            "type": "object",
            "properties": {
                "order_item_id": {
                    "type": "string",
                    "required": true,
                    "description" : "Jet's unique identifier for an item in a merchant order"
                },
                "merchant_sku": {
                    "type": "string",
                    "required": true,
                    "description" : "The merchant SKU that is in the given merchant order"
                },
                "order_quantity": {
                    "type": "integer",
                    "required": true,
                    "description" : "The quantity of the given merchant SKU that is in the given merchant order"
                },
                "Item_tax_code": {
                    "type": "string",
                    "required" : false,
                    "description" : "the tax code associated with the given merchant SKU

                    #Valid Values

                    The tax codes that you provided for the given merchant SKU"
                },
                "item_price": {
                    "$ref" : "#/definitions/item_price"
                },
                "item_fees": {
                    "type": "number",
                    "required" : true,
                    "description" : "The fees paid ot Jet.com for the given quantity of the given merchant SKU in the given merchant order"
                },
                "fee_adjustments": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/fee_adjustment"
                    }
                },
                "tax_info": {
                    "type" : "object",
                    "all_of": [
                {
                    "$ref": "#/definitions/Tax_Jurisdictions"                
                },
                {
                    "$ref": "#/definitions/Taxable_Amounts"
                },
                {
                    "$ref": "#/definitions/Non_Taxable_Amounts"                
                },
                {
                    "$ref": "#/definitions/Zero_Rated_Amounts"
                },
                {
                    "$ref": "#/definitions/Tax_Collected_Amounts"                
                },
                {
                    "$ref": "#/definitions/Tax_Rates"
                }
            ]
                },
                "product_title": {
                    "type": "string"
                },
                "url": {
                    "type": "string"
                }
            }
        },
        "order_detail": {
            "title": "order_detail",
            "type": "object",
            "properties": {
                "shipping_carrier": {
                    "type" : "string",
                    "required" : false,
                    "enum" : ["UPS", "FedEx", "USPS"], 
                    "description" : "The shipping carrier that is delivering the shipment 

                    #Valid Values

                    *'UPS'

                    *'FedEx'

                    *'USPS'"

                },
                "shipping_method": {
                    "$ref": "#/definitions/shipping_method"
                },
                "service_level" : {
                    "type" : "string",
                    "enum" : ["Standard","Expedited","Scheduled","NextDay","SecondDay"],
                    "required" : false,
                    "description" : "service level the customer will receive

                    #Valid Values

                   *'Standard'

                   *'Expedited'

                   *'Scheduled'

                   *'NextDay'

                   *'SecondDay'"
                },
                "ship_by": {
                    "type": "string",
                    "required": false,
                    "description" : "Expected date that the shipment will be tendered to the given carrier

                    #Logic

                    This will be calculated based on when the merchant order is sent the Merchant and their given settings for Fulfilment Time, and Order Cutoff data set at either the account level or shipping method level"
                },
                "delivery_by": {
                    "type": "string",
                    "required" : false,
                    "description" : "Date the customer has been promised delivery 

                    #Logic 

                    The ShippingMethod selected should provide delivery by this date as long as you meet shipping requirements you communicated during shipping set up.   If you use Jet provided labels and ship ontime you should meet this date.  If you use your own shipping labels you can choose to use a carrier other than the one suggested by ShippingMethod as long as the DeliveryByDate is met or exceeded."
                },
                
            }
        },
        "fee_adjustment": {
            "title": "Fee_Adjustment",
            "type": "object",
            "properties": {
                "adjustment_name": {
                    "type": "string",
                    "required" : false,
                    "description" : "Name of the discount being given"
                },
                "adjustment_type": {
                    "type": "string",
                    "required" : false,
                    "enum" : ["item","allocated basket"],
                    "description" : "The type of discount being given wether it is directly attributed to the item or allocated from a basket level discount

                    #Valid Values

                    *'item'

                    *'allocated basket'

                    "
                },
                "value": {
                    "type": "number",
                    "required" : false,
                    "description" : "Dollar amount of the adjustment"
                },
                
            }
        },
        "Tax_Jurisdictions" : {
            "Title" : "Tax_Jurisdictions",
            "Type" : "object",
            "properties" : {
                "Tax_Location_Code" : {
                    "type" : "string",
                    "required" : false,
                    "description" : "Tax location code for the jurisdiction in which tax was collected"
                },
                "Tax_City" : {
                    "Type" : "string",
                    "required" : false,
                    "description" :"Name of the city in which tax was collected"
                },
                "Tax_County" : {
                    "Type" : "string",
                    "required" : false,
                    "description" : "Name of the county that tax was collected"
                },
                "Tax_State" : {
                    "Type" : "string",
                    "required" : false,
                    "descrition" : "Two letter abreviation for the state in which tax was collected"
                }
            }
        },
        "Taxable_Amounts" : {
            "Title" : "Taxable_Amounts",
            "Type" : "object",
            "properties" : {
                "District_Tax_Amount" : {
                    "type" : "number",
                    "required" : false,
                    "description" : "Dollar amount deemed taxable at the district level"
                },
                "City_Tax_Amount" : {
                    "Type" : "number",
                    "required" : false,
                    "description" :"Dollar amount deemed taxable at the city level"
                },
                "County_Tax_Amount" : {
                    "Type" : "number",
                    "required" : false,
                    "description" : "Dollar amount deemed taxable at the county level"
                },
                "State_Tax_Amount" : {
                    "Type" : "number",
                    "required" : false,
                    "descrition" : "Dollar amount deemed taxable at the state level"
                }
            }
        }, 
        "Non_Taxable_Amounts" : {
            "Title" : "Non_Taxable_Amounts",
            "Type" : "object",
            "properties" : {
                "District_NonTaxable_Amount" : {
                    "type" : "number",
                    "required" : false,
                    "description" : "Dollar amount deemed nontaxable at the district level"
                },
                "City_NonTaxable_Amount" : {
                    "Type" : "number",
                    "required" : false,
                    "description" :"Dollar amount deemed nontaxable at the city level"
                },
                "County_NonTaxable_Amount" : {
                    "Type" : "number",
                    "required" : false,
                    "description" : "Dollar amount deemed nontaxable at the county level"
                },
                "State_NonTaxable_Amount" : {
                    "Type" : "number",
                    "required" : false,
                    "descrition" : "Dollar amount deemed nontaxable at the state level"
                }
            }
        },
        "Zero_Rated_Amounts" : {
            "Title" : "Zero_Rated_Amounts",
            "Type" : "object",
            "properties" : {
                "District_Zero_Rated_Amount" : {
                    "type" : "number",
                    "required" : false,
                    "description" : "The amount associated with a 0% tax location at the district level"
                },
                "City_Zero_Rated_Amount" : {
                    "Type" : "number",
                    "required" : false,
                    "description" :"The amount associated with a 0% tax location at the city level"
                },
                "County_Zero_Rated_Amount" : {
                    "Type" : "number",
                    "required" : false,
                    "description" : "The amount associated with a 0% tax location at the county level"
                },
                "State_Zero_Rated_Amount" : {
                    "Type" : "number",
                    "required" : false,
                    "descrition" : "The amount associated with a 0% tax location at the state level"
                }
            }
        },
        "Tax_Collected_Amounts" : {
            "Title" : "Tax_Collected_Amounts",
            "Type" : "object",
            "properties" : {
                "Distict_Tax_Collected" : {
                    "type" : "number",
                    "required" : false,
                    "description" : "Amount of tax collected at the district level"
                },
                "City_Tax_Collected" : {
                    "Type" : "number",
                    "required" : false,
                    "description" :"Amount of tax collected at the City level"
                },
                "County_Tax_Collected" : {
                    "Type" : "number",
                    "required" : false,
                    "description" : "Amount of tax collected at the county level"
                },
                "State_Tax_Collected" : {
                    "Type" : "number",
                    "required" : false,
                    "descrition" : "Amount of tax collected at the state level"
                }
            }
        },
        "Tax_Rates" : {
            "Title" : "Tax_Rates",
            "Type" : "object",
            "properties" : {
                "District_Tax_Rate" : {
                    "type" : "number",
                    "required" : false,
                    "description" : "District tax rate"
                },
                "City_Tax_Rate" : {
                    "Type" : "number",
                    "required" : false,
                    "description" :"City tax rate"
                },
                "County_Tax_Rate" : {
                    "Type" : "number",
                    "required" : false,
                    "description" : "County tax rate"
                },
                "State_Tax_Rate" : {
                    "Type" : "number",
                    "required" : false,
                    "descrition" : "State tax rate"
                }

            }
        },
        
        "shipping_method": {
            "title": "Shipping_Method",
            "type": "string",
            "required" : false,
            "description" : "the shipping method for the given shipment_id

            #Logic

            Only use shipping methods you have set up

            #Valid Values

            List of shipping methods the merchant has set up"
        },
        "item_price" : {
            "title" : "Item_price",
            "type" : "object",
            "properties" : {
                "base_price" : {
                    "type" : "number",
                    "required": true,
                    "description" : "The price for the given quantity of the given merchant SKU in the given merchant order EXCLUDING the allocated shipping, tax and shipping tax  "
                    },
                "item_tax" :{
                    "type" : "number",
                    "required": false,
                    "description" : "the tax being charged on the base_price if applicable  "
                    },
                "item_shipping_cost":{
                    "type" : "number",
                    "required": false,
                    "description" : "The shipping cost that is allocated to this order_item_id if applicable "
                    },
                "item_shipping_tax":{
                    "type" : "number",
                    "required": false,
                    "description" : "The tax on shipping cost that is allocated to this order_item_id if applicable  "
                    }

                }
            

        },
        "metadata": {
            "title": "Metadata",
            "type": "object",
            "properties": {
                "notified_on": {
                    "type": "string"
                },
                "state": {
                    "type": "object"
                },
                "history": {
                    "type": "array"
                }
            }
        }
    }
}